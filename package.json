{
  "name": "nft-collection-service",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf ./dist",
    "lint": "eslint .",
    "fix": "eslint --fix .",
    "start": "npm run build && node -r dotenv/config ./dist/bootstrap.js",
    "dev": "npm run build && cross-env NODE_ENV=script node -r dotenv/config ./dist/bootstrap.js",
    "cli": "npm run build && cross-env NODE_ENV=cli node -r dotenv/config ./dist/bootstrap.js",
    "cli-server": "npm run build && cross-env NODE_ENV=cli node --max-old-space-size=60000 -r dotenv/config ./dist/bootstrap.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mavriklabs/nft-collection-service.git"
  },
  "devDependencies": {
    "@types/bl": "^5.0.2",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^17.0.14",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "bl": "^5.0.0",
    "chalk": "^4.1.2",
    "dotenv": "^16.0.0",
    "emittery": "^0.10.1",
    "ethers": "^5.5.4",
    "file-type": "^16.5.3",
    "firebase-admin": "^10.0.2",
    "got": "^11.8.3",
    "it-reader": "^3.0.0",
    "mime-types": "^2.1.30",
    "p-queue": "^6.6.2",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.6.0"
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "none",
    "semi": true
  }
}
